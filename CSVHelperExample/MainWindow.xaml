<Window x:Class="CSVHelperExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSVHelperExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="data_Grid_Csv" AutoGenerateColumns="True"
              ItemsSource="{Binding}" Margin="10,72,10,10"/>
        <Label Content="Simple CSV Datagrid" HorizontalAlignment="Left" Margin="297,10,0,0" VerticalAlignment="Top" FontSize="22"/>

    </Grid>
</Window>
